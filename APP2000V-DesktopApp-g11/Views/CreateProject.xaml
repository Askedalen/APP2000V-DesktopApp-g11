<assets:AnimatedUserControl
             x:Class="APP2000V_DesktopApp_g11.Views.CreateProject"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:APP2000V_DesktopApp_g11.Views"
             xmlns:assets="clr-namespace:APP2000V_DesktopApp_g11.Assets"
             mc:Ignorable="d" 
             d:DesignHeight="1080" d:DesignWidth="1620">

    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Border 
            Grid.Column="0"
            Grid.Row="0"
            BorderThickness="0,0,0,1" 
            BorderBrush="Black"
            />

        <TextBlock
            Text="Create new project"
            FontSize="26"
            />

        <StackPanel
            Grid.Row="1"
            Orientation="Vertical"
            Width="400"
            HorizontalAlignment="Left"
            Margin="30"
            >
            <Label
                Content="Project _Name"
                Target="{Binding ElementName=ProjectNameInput}"
                FontSize="20"/>
            <TextBox
                x:Name="ProjectNameInput"
                FontSize="20"
                Margin="0, 0, 0, 30"/>
            <Label
                Content="Project _Description"
                Target="{Binding ElementName=ProjectDescInput}"
                FontSize="20"/>
            <TextBox
                x:Name="ProjectDescInput"
                FontSize="20"
                Margin="0, 0, 0, 30"/>
            <Label
                Content="Project _Start (yyyy-mm-dd)"
                Target="{Binding ElementName=ProjectStartInput}"
                FontSize="20"/>
            <DatePicker
                x:Name="ProjectStartPicker"
                FontSize="20"
                Margin="0, 0, 0, 30"/>
            <Label
                Content="Project D_eadline (yyyy-mm-dd)"
                Target="{Binding ElementName=ProjectDeadlineInput}"
                FontSize="20"/>
            <DatePicker
                x:Name="ProjectDeadlinePicker"
                FontSize="20"
                Margin="0, 0, 0, 30"/>
            <Label
                Content="Project _Manager"
                Target="{Binding ElementName=ChooseTaskList}"
                FontSize="20"/>
            <ComboBox
                    x:Name="ChooseProjectManager"
                    Width="400"
                    FontSize="20"
                    Background="White"
                    HorizontalAlignment="Left">
                <ComboBox.ItemTemplate>
                    <DataTemplate>
                        <TextBlock>
                            <Run Text="{Binding FirstName}"/>
                            <Run Text="{Binding LastName}"/>
                        </TextBlock>
                    </DataTemplate>
                </ComboBox.ItemTemplate>
            </ComboBox>
            <Button
                x:Name="RegisterProjectBtn"
                Content="Register project" 
                FontSize="22"
                Padding="10"
                Margin="0, 30, 0, 30"
                Click="RegisterProjectBtn_Click" Height="69" >
                
            </Button>
            <TextBlock
                x:Name="ConfirmationBox"
                FontSize="28"/>
            <Button
                x:Name="GoToProjectBtn"
                Content="Go to project" 
                Visibility="Collapsed"
                Height="80"
                FontSize="28"
                Padding="10"
                Margin="0, 30, 0, 30"
                Click="GoToProjectBtn_Click">
                <Button.Triggers>
                    <EventTrigger RoutedEvent="Button.GotFocus">
                        <BeginStoryboard>
                            <Storyboard
                                x:Name="AnimationStoryboard">
                                <DoubleAnimationUsingKeyFrames
                                Storyboard.TargetName="MainTranslate"
                                Storyboard.TargetProperty="Y"
                                Duration="0:0:1.5">
                                    <LinearDoubleKeyFrame Value="-150" KeyTime="0:0:0.25"/>
                                    <LinearDoubleKeyFrame Value="0" KeyTime="0:0:0.5"/>
                                    <LinearDoubleKeyFrame Value="200" KeyTime="0:0:0.75"/>
                                    <LinearDoubleKeyFrame Value="320" KeyTime="0:0:1"/>
                                    <LinearDoubleKeyFrame Value="300" KeyTime="0:0:1.5"/>
                                </DoubleAnimationUsingKeyFrames>
                                <DoubleAnimationUsingKeyFrames
                                BeginTime="0:0:1"
                                Storyboard.TargetName="ProjectLeaderTranslate"
                                Storyboard.TargetProperty="Y"
                                Duration="0:0:2">
                                    <LinearDoubleKeyFrame Value="300" KeyTime="0:0:0.25"/>
                                    <LinearDoubleKeyFrame Value="-200" KeyTime="0:0:0.5"/>
                                    <LinearDoubleKeyFrame Value="150" KeyTime="0:0:0.75"/>
                                    <LinearDoubleKeyFrame Value="-150" KeyTime="0:0:1"/>
                                    <LinearDoubleKeyFrame Value="75" KeyTime="0:0:1.25"/>
                                    <LinearDoubleKeyFrame Value="-150" KeyTime="0:0:1.5"/>
                                    <LinearDoubleKeyFrame Value="-90" KeyTime="0:0:1.75"/>
                                </DoubleAnimationUsingKeyFrames>
                                <DoubleAnimationUsingKeyFrames
                                BeginTime="0:0:3"
                                Storyboard.TargetName="ThumbsUpTranslate"
                                Storyboard.TargetProperty="Y"
                                Duration="0:0:2">
                                    <LinearDoubleKeyFrame Value="700" KeyTime="0:0:0" />
                                    <LinearDoubleKeyFrame Value="327" KeyTime="0:0:0.25" />
                                </DoubleAnimationUsingKeyFrames>
                                <DoubleAnimationUsingKeyFrames
                                BeginTime="0:0:6"
                                Storyboard.TargetName="MainTranslate"
                                Storyboard.TargetProperty="Y"
                                Duration="0:0:1.5">
                                    <LinearDoubleKeyFrame Value="280" KeyTime="0:0:0.15"/>
                                    <LinearDoubleKeyFrame Value="500" KeyTime="0:0:0.5"/>
                                    <LinearDoubleKeyFrame Value="1100" KeyTime="0:0:1"/>
                                </DoubleAnimationUsingKeyFrames>
                                <DoubleAnimation
                                Storyboard.TargetName="RightGearRotate"
                                Storyboard.TargetProperty="Angle"
                                From="0"
                                To="1080"
                                Duration="0:0:9"/>
                                <DoubleAnimation
                                Storyboard.TargetName="LeftGearRotate"
                                Storyboard.TargetProperty="Angle"
                                From="1080"
                                To="0"
                                Duration="0:0:9"/>
                            </Storyboard>
                        </BeginStoryboard>
                    </EventTrigger>
                </Button.Triggers>
            </Button>
            

        </StackPanel>
